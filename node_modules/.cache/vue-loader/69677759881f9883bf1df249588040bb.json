n\t\t\t\t<p></p>\r\n\t\t\t\t<input type=\"file\" name=\"photo\" @change=\"fileChanged\">\r\n\t\t\t\t<button @click=\"upload\">Upload</button>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t\t<div class=\"heading\">\r\n\t\t\t<div class=\"circle\">2</div>\r\n\t\t\t\t<h2>Edit/Delete an Page</h2>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t\t<div class=\"edit\">\r\n\t\t\t<div class=\"form\">\r\n\t\t\t\t<input v-model=\"findTitle\" placeholder=\"Search\">\r\n\t\t\t\t<div class=\"suggestions\" v-if=\"suggestions.length > 0\">\r\n\t\t\t\t\t<div class=\"suggestion\" v-for=\"s in suggestions\" :key=\"s.id\" @click=\"selectItem(s)\">{{s.title}}</div>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"upload\" v-if=\"findPage\">\r\n\t\t\t\t<input v-model=\"findPage.pageName\">\r\n\t\t\t\t<p></p>\r\n\t\t\t\t<textarea v-model=\"findPage.pageText\"></textarea>\r\n\t\t\t\t<p></p>\r\n\t\t\t\t<img :src=\"findPage.path\" />\r\n\t\t\t</div>\r\n\t\t\t<div class=\"actions\" v-if=\"findPage\">\r\n\t\t\t\t<button @click=\"deletePage(findPage)\">Delete</button>\r\n\t\t\t\t<button @click=\"editPage(findPage)\">Edit</button>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nexport default {\r\nname: 'Admin',\r\n\tdata() {\r\n\treturn {\r\n\t\tpageName: \"\",\r\n\t\tpageText: \"\",\r\n\t\tfile: null,\r\n\t\tpages: [],\r\n\t\tfindName: \"\",\r\n\t\tfindPage: null,\r\n    }\r\n  },\r\n  computed: {\r\n    suggestions() {\r\n      let pages = this.pages.filter(page => page.title.toLowerCase().startsWith(this.findName.toLowerCase()));\r\n      return pages.sort((a, b) => a.pageName > b.pageName);\r\n    }\r\n  },\r\n  created() {\r\n    this.getPages();\r\n  },\r\n  methods: {\r\n    fileChanged(event) {\r\n      this.file = event.target.files[0]\r\n    },\r\n\tasync upload() {\r\n\t\ttry {\r\n\t\t\tconst formData = new FormData();\r\n\t\t\tformData.append('photo', this.file, this.file.name)\r\n\t\t\tlet r1 = await axios.post('/api/photos', formData);\r\n\t\t\tlet r2 = await axios.post('/api/items', {\r\n\t\t\t\tpageName: this.pageName,\r\n\t\t\t\tpageText: this.pageText,\r\n\t\t\t\tpath: r1.data.path\r\n\t\t\t});\r\n\t\t\tthis.getPages();\r\n\t\t} catch (error) {\r\n\t\t\tconsole.log(error);\r\n\t\t}\r\n\t},\r\n\tasync getPages() {\r\n\t\ttry {\r\n\t\t\tlet response = await axios.get(\"/api/pages\");\r\n\t\t\tthis.pages = response.data;\r\n\t\t\treturn true;\r\n\t\t} catch (error) {\r\n\t\t\tconsole.log(error);\r\n\t\t}\r\n\t\t},\r\n\tselectPage(page) {\r\n      this.findName = \"\";\r\n      this.findPage = page;\r\n    },\r\n\tasync deletePage(page) {\r\n      try {\r\n        await axios.delete(\"/api/pages/\" + page.pageName);\r\n        this.findPage = null;\r\n        this.getPages();\r\n        return true;\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n\tasync editPage(page) {\r\n      try {\r\n        await axios.put(\"/api/pages/\" + page.pageName, {\r\n\t\t\tpageName: this.findPage.pageName,\r\n\t\t\tpageText: this.findItem.pageText\r\n        });\r\n        this.findPage = null;\r\n        this.getPages();\r\n        return true;\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.image h2 {\r\n  font-style: italic;\r\n  font-size: 1em;\r\n}\r\n\r\n.heading {\r\n  display: flex;\r\n  margin-bottom: 20px;\r\n  margin-top: 20px;\r\n}\r\n\r\n.heading h2 {\r\n  margin-top: 8px;\r\n  margin-left: 10px;\r\n}\r\n\r\n.add,\r\n.edit {\r\n  display: flex;\r\n}\r\n\r\n.circle {\r\n  border-radius: 50%;\r\n  width: 18px;\r\n  height: 18px;\r\n  padding: 8px;\r\n  background: #333;\r\n  color: #fff;\r\n  text-align: center\r\n}\r\n\r\n/* Form */\r\ninput,\r\ntextarea,\r\nselect,\r\nbutton {\r\n  font-family: 'Montserrat', sans-serif;\r\n  font-size: 1em;\r\n}\r\n\r\n.form {\r\n  margin-right: 50px;\r\n}\r\n\r\n/* Uploaded images */\r\n.upload h2 {\r\n  margin: 0px;\r\n}\r\n\r\n.upload img {\r\n  max-width: 